#include <stdio.h>

/**
 * main - a program that multiplies two numbers
 * @argc: Number of Argument passed
 * @argv: string of argument
 * Return: success
 */
int main(int argc, char *argv[])
{
	if (argc == 3)
	{
		int num1 = atoi(argv[1]);
		int num2 = atoi(argv[2]);
		int mult = num1 * num2;

		printf("%d\n", mult);
		return (0);
	}
	else
	{
		printf("Error\n");
		return (1);
	}
}
