#include "lists.h"

/**
 * free_dlistint - a function that frees a doubly linked list
 * @head: to be freed
 * Return: success
 */
void free_dlistint(dlistint_t *head)
{
	dlistint_t *current;

	while (head != NULL)
	{
		current = head;
		head = head->next;
		free(current->prev);
		free(current);
	}
}
