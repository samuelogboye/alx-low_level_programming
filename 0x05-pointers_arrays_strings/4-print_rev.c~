#include "main.h"

/**
 * print_rev - a function that prints a string, in reverse, followed
 * by a new line
 * @s: input
 * Return: 0 Success
 */
void print_rev(char *s)
{
	int i, j;
	char c;
	int len = _strlen(s);

	for (i = 0, j = len - 1; i < j; i++, j--)
	{
		c = s[i];
		s[i] = s[j];
		s[j] = c;
	}
	_putchar(*s);
	_putchar('\n');
}
