#include "main.h"

/**
 * print_square - prints a square on the terminal
 * @size: of the square
 * good
 */
void print_square(int size)
{
	int i, j;

	if (size <= 0)
	{
		_putchar('\n');
		return;
	}
	for (j = 1; j < size; j++)
	{
		_putchar('#');

		for (i = 2; i < size; i++)
		{
			_putchar('#');
		}
		_putchar('\n');
	}
}
